<?php   

// open Document
$handle = fopen("php://stdin", "r");

//take number first line and remove whitespace
$line = trim(fgets($handle), " ");

// get first character of first line
$_n = $line[0];

// get secon character of first line
$_t = $line[1];

// get second line and remove Whitespace
$line = trim(fgets($handle, " ");

// make Array and fill it with Data from second line
$position = array();
for ($c = 0 ; $c <= strlen($line)-1 ; $c++){
    array_push($position, $line[$c];);
}

?>
